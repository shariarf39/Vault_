                android:textColor="@color/blueclr"
                android:textSize="@dimen/display_secondary_text_port"
                android:fontFamily="@font/ndotfont"
                />

            <Space
                android:layout_width="fill_parent"
                android:layout_height="0.0dip"
                android:layout_weight="3.0" />
        </LinearLayout>

